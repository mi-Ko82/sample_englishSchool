{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "baseUrl": "./"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.js"]
}